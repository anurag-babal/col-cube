<?xml version="1.0" encoding="UTF-8"?>
<cubestore xmlns="http://www.example.com/cubestore"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.example.com/cubestore cubestore.xsd">

  <tablename name="sales" type="fact">
    <attributes>category</attributes>
    <attributes>sales</attributes>
    <attributes>profit</attributes>
    <attributes>cust_id</attributes>
    <attributes>prod_id</attributes>
  </tablename>

  <tablename name="cust" type="dim">
    <attributes>id</attributes>
    <attributes>state</attributes>
    <attributes>city</attributes>
    <attributes>name</attributes>
  </tablename>

  <tablename name="prod" type="dim">
    <attributes>id</attributes>
    <attributes>category</attributes>
    <attributes>sub_category</attributes>
  </tablename>

</cubestore>
